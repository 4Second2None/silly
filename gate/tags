LDFLAG	Makefile	/^LDFLAG = $/;"	m
OBJS	Makefile	/^OBJS = $(patsubst %.c,%.o,$(SRC))$/;"	m
P	ppoll.c	/^struct ppoll *P;$/;"	v	typeref:struct:ppoll
